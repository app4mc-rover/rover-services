#%RAML 1.0
title: Rover Infrared Sensor
mediaType: application/json
version: v1
baseUri: localhost:8000/api?token=x
/read:
  description: "Read Data From Sensor"
  post:
    body:
      properties:
        sensor_id:
          type: integer
  get:
    responses:
      200:
        body:
          properties:
            speed:
              type: number
